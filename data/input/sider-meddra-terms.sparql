PREFIX sider: <http://bio2rdf.org/sider_vocabulary:>
#SELECT distinct ?umlsid ?parent_1 ?parent_2 ?parent_3
SELECT *
WHERE {
#?sider_drug sider:indication ?i .
?a <http://bioportal.bioontology.org/ontologies/umls/cui> ?i .
?i rdfs:label ?c .
?a rdfs:subClassOf ?parent_1 .
OPTIONAL
{
?parent_1 rdfs:subClassOf ?parent_2 .
?parent_2 rdfs:subClassOf ?parent_3 .
}
FILTER (regex(str(?a),"^http://bio2rdf.org/meddra:"))
BIND (STRAFTER(str(?i),"http://bio2rdf.org/umls:") AS ?umlsid)
}
LIMIT 1000
